{
  "name": "jQueryGantt",
  "version": "1.0.0",
  "description": "jQuery Gantt editor",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "CSC_IDENTITY_AUTO_DISCOVERY=false && electron-builder --dir",
    "dist": "CSC_IDENTITY_AUTO_DISCOVERY=false && electron-builder"
  },
  "build": {
    "appId": "com.electron.jquerygantt.modified",
    "mac": {
      "category": "your.app.category.type"
    },
    "linux": {
      "target": "rpm",
      "category": "your.app.category.type"
    },
    "win": {
      "target": "portable"
    }
  },
  "repository": "https://github.com/robicch/jQueryGantt.git",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "MIT",
  "devDependencies": {
    "electron": "3.0.4",
    "electron-builder": "^20.31.2"
  },
  "dependencies": {
    "jquery": "3.1.1",
    "jquery-ui-dist": "1.12.1"
  }
}
